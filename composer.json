{
    "name": "ace/zend-money",
    "description": "Helpful form and validation stuff for zend's form and moneyphp",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "George Steel",
            "email": "george@net-glue.co.uk"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "laminas/laminas-dependency-plugin": "^1.0 || ^2.0",
        "laminas/laminas-filter": "^2.9",
        "laminas/laminas-form": "^2.16",
        "laminas/laminas-hydrator": "^3 || ^4",
        "laminas/laminas-i18n": "^2.9",
        "laminas/laminas-inputfilter": "^2.10",
        "laminas/laminas-stdlib": "^3.2",
        "laminas/laminas-validator": "^2.13",
        "moneyphp/money": "^3.2",
        "psr/container": "^1.0 || ^2.0"
    },
    "require-dev": {
        "doctrine/coding-standard": "^8.2 || ^9.0",
        "laminas/laminas-code": "^3.3",
        "laminas/laminas-config-aggregator": "^1.1",
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "ACE\\Money\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ACETest\\Money\\": "test/Money"
        }
    }
}
